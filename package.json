{
  "name": "@trimbleinc/modus-react-bootstrap",
  "version": "1.1.4",
  "description": "The Modus React Bootstrap is a component library extended from Open Source library react-bootstrap developed as a common, open source platform for all of Trimbleâ€™s web applications built on React.",
  "keywords": [
    "modus",
    "react",
    "react-bootstrap",
    "modus-react-bootstrap",
    "modus-bootstrap"
  ],
  "homepage": "https://modus-react-bootstrap.trimble.com/",
  "license": "MIT",
  "author": "Trimble Inc.",
  "files": [
    "CHANGELOG.md",
    "lib",
    "dist",
    "es",
    "scss"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:trimble-oss/modus-react-bootstrap.git"
  },
  "scripts": {
    "bootstrap": "yarn --network-timeout 100000 && yarn --cwd sample-workspace --network-timeout 100000",
    "build": "yarn run format && yarn run lint && node tools/build.js && node tools/custom.js && yarn run build-css",
    "build-css": "yarn run css-lint && yarn run css-minify && copyfiles -a ./scss/**/* lib/css/",
    "build-sample-workspace": "yarn --cwd sample-workspace build",
    "build-types": "yarn tsc -d --emitDeclarationOnly --outDir types",
    "css-compile": "node-sass scss/modus-react-bootstrap.scss lib/css/dist/modus-react-bootstrap.css && node-sass scss/modus-react-bootstrap-dark.scss lib/css/dist/modus-react-bootstrap-dark.css && postcss lib/css/dist/*.css --config ./ --verbose --use autoprefixer -r",
    "css-lint": "stylelint \"scss/**/*.scss\" --fix",
    "css-minify": "yarn run css-compile && cleancss -O1 --output lib/css/dist/modus-react-bootstrap.min.css lib/css/dist/modus-react-bootstrap.css && cleancss -O1 --output lib/css/dist/modus-react-bootstrap-dark.min.css lib/css/dist/modus-react-bootstrap-dark.css",
    "format": "eslint --ext tsx --ext ts src --fix",
    "lint": "eslint --ext tsx --ext ts src && tsc --noEmit",
    "prepublishOnly": "yarn run build",
    "release": "rollout",
    "start": "yarn run build && yarn --cwd sample-workspace start"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --fix"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@restart/context": "^2.1.4",
    "@restart/hooks": "^0.4.7",
    "@trimbleinc/modus-bootstrap": "^1.5.4",
    "@types/invariant": "^2.2.35",
    "@types/prop-types": "^15.7.5",
    "@types/react": ">=16.14.8",
    "@types/react-transition-group": "^4.4.1",
    "@types/warning": "^3.0.0",
    "autoprefixer": "^10.4.9",
    "classnames": "^2.3.1",
    "clean-css-cli": "^5.6.1",
    "copyfiles": "^2.4.1",
    "dom-helpers": "^5.2.1",
    "invariant": "^2.2.4",
    "node-sass": "^7.0.3",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "prop-types": "^15.8.1",
    "prop-types-extra": "^1.1.1",
    "react-overlays": "^5.1.2",
    "react-table": "^7.8.0",
    "react-transition-group": "^4.4.1",
    "shx": "^0.3.4",
    "styled-components": "^5.3.5",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^5.0.0",
    "uncontrollable": "^7.2.1",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@4c/rollout": "^3.0.1",
    "@4c/tsconfig": "^0.4.0",
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.18.9",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.17.7",
    "@react-bootstrap/babel-preset": "^2.1.0",
    "@react-bootstrap/eslint-config": "^2.0.0",
    "@types/react-table": "^7.7.12",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^4.3.6",
    "chalk": "4.1.2",
    "cherry-pick": "^0.5.0",
    "codecov": "^3.8.2",
    "conventional-changelog-cli": "^2.2.2",
    "cpy-cli": "^4.1.0",
    "cross-env": "^7.0.3",
    "dtslint": "^4.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^8.20.0",
    "eslint-config-4catalyzer-typescript": "^3.2.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-mocha": "^10.0.5",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0-next-327e4a1f9-20220613",
    "execa": "^5.0.0",
    "fs-extra": "^10.1.0",
    "husky": "^7.0.4",
    "karma": "^6.3.17",
    "karma-chrome-launcher": "^3.1.1",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^2.2.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
    "lint-staged": "^12.3.7",
    "lodash": "^4.17.21",
    "mocha": "^9.2.2",
    "prettier": "^2.6.2",
    "process": "^0.11.10",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^16.14.0",
    "simulant": "^0.2.2",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "stream-browserify": "^3.0.0",
    "typescript": "^4.7.3",
    "util": "^0.12.4",
    "webpack": "^5.72.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "publishConfig": {
    "directory": "lib"
  },
  "release": {
    "conventionalCommits": true
  },
  "engines": {
    "node": ">=14"
  },
  "resolutions": {
    "styled-components": "^5",
    "fast-glob": "^3.2.11"
  }
}
